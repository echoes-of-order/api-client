{
  "name": "@echoes-of-order/api-client",
  "version": "2.4.0",
  "description": "Shared API client for Echoes of Order projects",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/echoes-of-order/api-client.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:run": "vitest run",
    "lint": "eslint src",
    "prepublishOnly": "yarn build",
    "release:patch": "yarn version patch",
    "release:minor": "yarn version minor",
    "release:major": "yarn version major"
  },
  "keywords": [
    "api",
    "client",
    "http",
    "echoes-of-order"
  ],
  "author": "Echoes of Order Team",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "src"
  ],
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  }
}
